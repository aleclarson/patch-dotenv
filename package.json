{
  "name": "patch-dotenv",
  "description": "Patch a .env file in place, preserving the existing formatting.",
  "type": "module",
  "version": "0.1.0",
  "bin": {
    "patch-dotenv": "./dist/main.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Alec Larson",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleclarson/patch-dotenv.git"
  },
  "prettier": "@alloc/prettier-config",
  "scripts": {
    "prepublishOnly": "pnpm build",
    "dev": "rimraf dist && tsup --sourcemap --watch",
    "build": "rimraf dist && tsup",
    "lint": "tsc -p . --outDir node_modules/.tmp",
    "test": "vitest"
  },
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
